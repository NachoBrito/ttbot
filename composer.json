{
    "name": "nachobrito/ttbot",
    "type": "project",
    "license": "MIT",
    "description": "Text Tools TwitterBot",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },    
    "autoload": {
        "psr-4": {
            "NachoBrito\\TTBot\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "html2text/html2text": "^4.3",
        "landrok/language-detector": "^1.3",
        "php-science/textrank": "^1.2",
        "symfony/config": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/finder": "^5.4",
        "symfony/messenger": "^5.4"
    }
}
